{
  "version": 2,
  "builds": [
    {
      "src": "build/web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/manifest.json",
      "dest": "/build/web/manifest.json"
    },
    {
      "src": "/flutter_bootstrap.js",
      "dest": "/build/web/flutter_bootstrap.js"
    },
    {
      "src": "/main.dart.js",
      "dest": "/build/web/main.dart.js"
    },
    {
      "src": "/(.*\\.(js|css|json|wasm|png|jpg|jpeg|svg|ico|ttf|otf|woff|woff2))",
      "dest": "/build/web/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/web/index.html"
    }
  ]
}
